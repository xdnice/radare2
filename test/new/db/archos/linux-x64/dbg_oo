NAME=dbg.oo 10
FILE=/bin/ls
ARGS=-d
BROKEN=1
CMDS=<<EOF
10oo
e file.path
dk 9
EOF
EXPECT=<<EOF
dbg:///bin/ls
EOF
RUN

NAME=dbg.ood 10
FILE=/bin/ls
ARGS=-d
BROKEN=1
CMDS=<<EOF
10ood
e file.path
dk 9
EOF
EXPECT=<<EOF
dbg:///bin/ls
EOF
RUN

NAME= ood after open in debug mode
FILE=../bins/elf/analysis/x86-helloworld-gcc
ARGS=-d
CMDS=<<EOF
ood > $_
dc
ood > $_
dc
EOF
EXPECT=<<EOF
Hello world!
Hello world!
EOF
RUN

NAME=oo ; dc
FILE=../bins/elf/analysis/x86-helloworld-gcc
ARGS=-d
CMDS=<<EOF
oo
dc
EOF
EXPECT=<<EOF
Hello world!
EOF
RUN
